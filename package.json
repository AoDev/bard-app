{
  "name": "rs-bard",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "check": "biome check --write",
    "dev": "rsbuild dev --open",
    "format": "biome format --write src && biome lint --write src",
    "lint:fix": "biome check --write .",
    "lint": "biome check --diagnostic-level error && tsc --build --noEmit",
    "preview": "rsbuild preview",
    "test": "vitest --config vitest.config.ts --run",
    "test:watch": "vitest --config vitest.config.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@floating-ui/react": "0.27.6",
    "@rsbuild/core": "1.4.6",
    "@rsbuild/plugin-less": "1.2.5",
    "@rsbuild/plugin-react": "1.3.4",
    "@testing-library/react": "16.3.0",
    "@types/lodash": "4.17.20",
    "@types/node": "22.14.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-syntax-highlighter": "15.5.13",
    "axios": "1.10.0",
    "bard-router": "3.0.0",
    "chokidar": "4.0.3",
    "clipboard": "2.0.11",
    "fast-glob": "3.3.3",
    "jsdom": "26.1.0",
    "localforage": "1.10.0",
    "lodash": "4.17.21",
    "mobx": "6.13.7",
    "mobx-react": "9.2.0",
    "prettier": "3.6.2",
    "prism-react-renderer": "2.4.1",
    "rc-switch": "4.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "10.1.0",
    "react-syntax-highlighter": "15.6.1",
    "rehype-raw": "7.0.0",
    "remark-gfm": "4.0.1",
    "rsbuild-plugin-svg-icons": "0.1.1",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  }
}
